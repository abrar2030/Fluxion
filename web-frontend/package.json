{
  "name": "fluxion-frontend",
  "version": "1.0.0",
  "description": "Modern UI for Fluxion DeFi Platform",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
